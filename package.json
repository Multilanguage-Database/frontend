{
  "name": "frontend",
  "type": "module",
  "version": "0.0.1",
  "scripts": {
    "start": "astro dev",
    "lint": "concurrently -c 'auto' 'npm:lint:*'",
    "lint:eslint": "pnpm eslint src/",
    "lint:prettier": "pnpm prettier --list-different --plugin-search-dir=. .",
    "lint:types": "tsc",
    "lint:astro": "astro check",
    "fixlint": "concurrently -c 'auto' 'npm:fix:*'",
    "fix:eslint": "pnpm eslint src/ --fix;",
    "fix:prettier": "pnpm prettier --write --plugin-search-dir=. .",
    "build": "astro check && tsc && astro build",
    "preview": "astro preview",
    "astro": "astro"
  },
  "dependencies": {
    "@tailwindcss/typography": "0.5.9",
    "astro": "2.3.0",
    "axios": "1.3.5"
  },
  "devDependencies": {
    "@astrojs/react": "2.1.1",
    "@astrojs/tailwind": "3.1.1",
    "@types/node": "18.15.11",
    "@types/react": "18.0.37",
    "@types/react-dom": "18.0.11",
    "@typescript-eslint/eslint-plugin": "5.58.0",
    "@typescript-eslint/parser": "5.58.0",
    "astro-eslint-parser": "0.13.3",
    "autoprefixer": "10.4.14",
    "concurrently": "8.0.1",
    "daisyui": "2.51.5",
    "eslint": "8.38.0",
    "eslint-config-prettier": "8.8.0",
    "eslint-config-standard-with-typescript": "34.0.1",
    "eslint-import-resolver-typescript": "3.5.5",
    "eslint-plugin-astro": "0.26.1",
    "eslint-plugin-import": "2.27.5",
    "eslint-plugin-n": "15.7.0",
    "eslint-plugin-promise": "6.1.1",
    "eslint-plugin-react": "7.32.2",
    "eslint-plugin-simple-import-sort": "10.0.0",
    "eslint-plugin-tailwindcss": "3.11.0",
    "postcss": "8.4.21",
    "prettier": "2.8.7",
    "prettier-plugin-astro": "0.8.0",
    "prettier-plugin-tailwindcss": "0.2.7",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "tailwindcss": "3.3.1",
    "typescript": "5.0.4"
  },
  "engines": {
    "node": ">=18.0.0 <19.0.0",
    "pnpm": ">=8.0.0",
    "npm": "please-use-pnpm",
    "yarn": "please-use-pnpm"
  }
}
